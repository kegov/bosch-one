{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Nicla Debug (PyOCD)",
            "cwd": "${workspaceFolder}",
            "executable": "./build/zephyr/zephyr.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "pyocd",
            "targetId": "nrf52",
            "device": "nrf52832",
            "svdFile": "${env:UserProfile}/ncs/v2.3.0/modules/hal/nordic/nrfx/mdk/nrf52.svd",
        }
    ]
}